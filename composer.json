{
    "name": "marekskopal/typo3-reference",
    "type": "typo3-cms-extension",
    "description": "This TYPO3 extension provides a reference portfolio feature, allowing you to showcase projects or works in a structured manner.",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.2",
        "clickstorm/go-maps-ext": "^7.1",
        "typo3/cms-backend": "^13.4",
        "typo3/cms-core": "^13.4",
        "typo3/cms-extbase": "^13.4"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.0",
        "shipmonk/phpstan-rules": "^4.0",
        "slevomat/coding-standard": "^8.14"
    },
    "autoload": {
        "psr-4": {
            "MarekSkopal\\MsReference\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ms_reference"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
