<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.sheet_general</sheetTitle>
                <type>array</type>
                <el>
                    <settings.listCategories>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_categories</label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <size>5</size>
                            <maxitems>10</maxitems>
                            <allowed>tx_msreference_domain_model_category</allowed>
                            <foreign_table>tx_msreference_domain_model_category</foreign_table>
                        </config>
                    </settings.listCategories>

                    <settings.listGroupByCategories>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_group_by_categories</label>
                        <config>
                            <type>check</type>
                        </config>
                    </settings.listGroupByCategories>

                    <settings.gMaps>
                      <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.gMaps</label>
                      <config>
                        <type>check</type>
                      </config>
                    </settings.gMaps>

                    <settings.showPageUid>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.show_page_uid</label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <relationship>manyToOne</relationship>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <suggestOptions>
                                <default>
                                    <addWhere>AND pages.doktype NOT in (6,199,254)</addWhere>
                                </default>
                            </suggestOptions>
                        </config>
                    </settings.showPageUid>

                    <settings.templateLayout>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.template_layout</label>
                        <config>
                            <type>select</type>
                            <itemsProcFunc>MarekSkopal\Reference\Hooks\ItemsProcFunc->templateLayout</itemsProcFunc>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0" type="array">
                                    <label/>
                                    <value/>
                                </numIndex>
                            </items>
                        </config>
                    </settings.templateLayout>

                    <settings.listLimit>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_limit</label>
                        <config>
                            <type>number</type>
                            <format>integer</format>
                            <nullable>1</nullable>
                            <default>null</default>
                        </config>
                    </settings.listLimit>

                    <settings.listSortField>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_sort_field</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0" type="array">
                                    <label>Pořadí</label>
                                    <value>sorting</value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>Id</label>
                                    <value>uid</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>Názvu</label>
                                    <value>title</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>Data vytvoření</label>
                                    <value>crdate</value>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <label>Data realizace</label>
                                    <value>realization_date</value>
                                </numIndex>
                                <numIndex index="5" type="array">
                                    <label>Ruční</label>
                                    <value>custom</value>
                                </numIndex>
                            </items>
                        </config>
                    </settings.listSortField>
                    <settings.listSortDir>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_sort_dir</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0" type="array">
                                    <label index="0">Vzestupně</label>
                                    <value>asc</value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label index="0">Sestupně</label>
                                    <value>desc</value>
                                </numIndex>
                            </items>
                        </config>
                    </settings.listSortDir>
                    <settings.listSortImportant>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_sort_important</label>
                        <config>
                            <type>check</type>
                        </config>
                    </settings.listSortImportant>
                    <settings.listReference>
                        <label>LLL:EXT:ms_reference/Resources/Private/Language/locallang_db.xlf:flexform.list_reference</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingleBox</renderType>
                            <foreign_table>tx_msreference_domain_model_reference</foreign_table>
                            <foreign_table_where> AND tx_msreference_domain_model_reference.sys_language_uid IN (-1, 0) ORDER BY tx_msreference_domain_model_reference.title ASC</foreign_table_where>
                            <size>10</size>
                            <minitems>0</minitems>
                            <maxitems>20</maxitems>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                    <maxItemsInResultList>1</maxItemsInResultList>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.listReference>
                 </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
